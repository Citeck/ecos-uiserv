{
    "id": "upload-new-version",
    "key": "upload.new.version",
    "name": {
        "ru": "Загрузить новую версию",
        "en": "Upload New Version"
    },
    "type": "upload-new-version",
    "evaluator": {
        "type": "group",
        "config": {
            "joinBy": "AND",
            "evaluators": [
                {
                    "type": "has-permission",
                    "config": {
                        "permission": "Write"
                    }
                },
                {
                    "type": "predicate",
                    "config": {
                        "predicate": {
                            "t": "eq",
                            "att": ".att(n:\"dict\"){has(n:\"cm:content\")}",
                            "val": true
                        }
                    }
                }
            ]
        }
    }
}
