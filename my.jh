entity MenuConfig {
  menuId String required
}

entity MenuConfigVersion {
  xml AnyBlob required
}

relationship OneToOne {
  MenuConfig to MenuConfigVersion
}

relationship ManyToOne {
  MenuConfigVersion to MenuConfig
}

entity Translated {
}

entity Translation {
  langTag String required
  bundle AnyBlob required
}

relationship OneToOne {
  MenuConfigVersion to Translated
}

relationship ManyToOne {
  Translation to Translated
}
